{
  "name": "hagicode-desktop",
  "productName": "Hagicode Desktop",
  "version": "0.1.0",
  "type": "module",
  "description": "Desktop client for Hagicode Server management and monitoring",
  "main": "dist/main/main.js",
  "scripts": {
    "build:tsc": "npx tsc",
    "build:preload": "vite build --config vite.preload.config.ts",
    "build:tsc:check": "npx tsc --noEmit",
    "build:renderer": "vite build",
    "build:all": "npm run build:tsc && npm run build:preload && npm run build:renderer",
    "build:check": "npm run build:tsc:check && npm run build:renderer",
    "smoke-test": "node scripts/smoke-test.js",
    "smoke-test:verbose": "node scripts/smoke-test.js --verbose",
    "build:prod": "npm run build:all && npm run smoke-test",
    "dev:renderer": "vite",
    "dev:main": "tsc --watch",
    "dev:preload": "vite build --config vite.preload.config.ts --watch",
    "dev": "rm -rf dist && concurrently -k \"npm run dev:renderer\" \"npm run dev:main\" \"npm run dev:preload\" \"wait-on dist/preload/index.mjs && wait-on dist/main/main.js && cross-env NODE_ENV=development HAGICO_CONFIG_PATH=local_data_root electron .\"",
    "start": "npm run build:all && electron .",
    "prebuild": "npm run build:all",
    "build": "electron-builder",
    "build:win": "npm run build:prod && electron-builder --win",
    "build:appx": "npm run build:prod && electron-builder --win --appx",
    "build:appx:prod": "npm run build:prod && electron-builder --win --appx --publish never",
    "build:mac": "npm run build:prod && electron-builder --mac",
    "build:linux": "npm run build:prod && electron-builder --linux",
    "dist": "npm run build:prod && electron-builder",
    "version:bump": "node scripts/bump-version.js",
    "build:local": "npm run version:bump && npm run build:prod && electron-builder --config electron-builder.local.js",
    "build:local:publish": "npm run version:bump && npm run build:prod && electron-builder --config electron-builder.local.js --publish always"
  },
  "keywords": [
    "hagicode",
    "desktop",
    "server",
    "management",
    "monitoring"
  ],
  "author": {
    "name": "newbe36524",
    "email": "support@hagicode.com"
  },
  "homepage": "https://github.com/HagiCode-org/desktop",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/adm-zip": "^0.5.7",
    "@types/ini": "^4.1.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@types/react-window": "^1.8.8",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-react": "^4.3.4",
